    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "timezone": "America/New_York",
    "separateMinorPatch": true,
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,
    "baseBranches": ["main"],
    "extends": [":dependencyDashboard"],
    "additionalBranchPrefix": "{{parentDir}}-",
    "recreateWhen": "always"
}
