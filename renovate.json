
{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    
    {"fileMatch": [
      "k8s/.+\\.yaml$",
      "crossplane-gcp-provider/.+/provider\\.yaml$"
    ],
    "matchStrings": [
      "package: xpkg\\.upbound\\.io\\/upbound\\/provider-gcp-container:(?<currentValue>v \\d\\.\\d+\\.\\d+?) # repository=(?<depName>https:\\/\\/github\\.com\\/upbound\\/provider-gcp?)"
    ],
    
    "matchStringsStrategy": "any",
    "datasourceTemplate": "git-tags"
  }
  ]

}
